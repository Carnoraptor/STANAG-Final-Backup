<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "../bulletml.dtd">

<bulletml type="vertical"
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

<action label="top">
<repeat>
<times>8</times>
<actionRef label="oneburst"></actionRef>
<wait>30</wait>
</repeat>

</action>


<action label="oneburst">
<repeat>
 <times>20</times>
  <actionRef label="circle1"> </actionRef>
</repeat>
</action>


<action label="circle1">
  <fire>
    <bullet label="SpikeBullet">
    <direction type="sequence">15 + (25 * $rand)</direction>
    <speed type="absolute">0.4 * $rand + 0.2</speed>
    </bullet>
  </fire>

</action>

</bulletml>