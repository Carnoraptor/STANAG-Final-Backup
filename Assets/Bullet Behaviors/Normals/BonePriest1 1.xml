<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "../bulletml.dtd">

<bulletml type="vertical"
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

<action label="top">
<repeat>
<times>45</times>
<actionRef label="oneburst"></actionRef>
</repeat>

</action>


<action label="oneburst">
<repeat>
 <times>5</times>
  <actionRef label="circle1"> </actionRef>
</repeat>
<wait>30</wait>
</action>


<action label="circle1">
  <fire>
    <bullet label="SpikeBullet">
    <direction type="aim">50 - (100 * $rand)</direction>
    <speed type="absolute">1.2</speed>
    </bullet>
  </fire>

</action>

</bulletml>